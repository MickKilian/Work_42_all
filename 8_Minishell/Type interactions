command_1 && command_2
executes command_2 only if command_1 was successfull

command_1 || command_2
executes command_2 only if command_1 failed


SEP_CAR
STD_CAR 
SIMPLE_CAR
DOUBLE_CAR
PIPE_CAR
AMP_CAR
LT_CAR  
GT_CAR   

NEW_STD
CONT_STD
NEW_SIMPLE
CONT_SIMPLE
NEW_DOUBLE
CONT_DOUBLE
REDIR_IN
HEREDOC
REDIR_OUT
REDIR_OUT_APP

LEXER_FUNCTIONS:
#NEXT
#IGNORE
#SYNTAX_ERROR

RECORD MODES :
NEW_REC
CONT_REC
IGNORE_REC (if CONT->NEW then NEXT)

NEW_STD   | SPE_CAR   -> NEW_STD
NEW_STD   | STD_CAR   -> CONT_STD
NEW_STD   | SIMPLE_CAR-> NEW_SIMPLE
NEW_STD   | DOUBLE_CAR-> NEW_DOUBLE
NEW_STD   | PIPE_CAR  -> SYNTAX ERROR
NEW_STD   | AMP_CAR   -> CONT_STD
NEW_STD   | LT_CAR    -> REDIR_IN_NEW
NEW_STD   | GT_CAR    -> REDIR_OUT_NEW

CONT_STD  | SPE_CAR   -> #NEXT + NEW_STD
CONT_STD  | STD_CAR   -> CONT_STD
CONT_STD  | SIMPLE_CAR-> #IGNORE + CONT_SIMPLE
CONT_STD  | DOUBLE_CAR-> #IGNORE + CONT_DOUBLE
CONT_STD  | PIPE_CAR  -> #NEXT + PIPE 
CONT_STD  | AMP_CAR   ->
CONT_STD  | LT_CAR    -> #NEXT + REDIR_IN_NEW
CONT_STD  | GT_CAR    -> #NEXT + REDIR_OUT_NEW

NEW_SIMPLE| SPE_CAR   -> CONT_SIMPLE
NEW_SIMPLE| STD_CAR   -> CONT_SIMPLE
NEW_SIMPLE| SIMPLE_CAR-> #IGNORE + #NEXT + NEW_STD
NEW_SIMPLE| DOUBLE_CAR-> CONT_SIMPLE
NEW_SIMPLE| PIPE_CAR  -> CONT_SIMPLE
NEW_SIMPLE| AMP_CAR   -> CONT_SIMPLE
NEW_SIMPLE| LT_CAR    -> CONT_SIMPLE
NEW_SIMPLE| GT_CAR    -> CONT_SIMPLE

CONT_SIMPLE| SPE_CAR   -> CONT_SIMPLE
CONT_SIMPLE| STD_CAR   -> CONT_SIMPLE
CONT_SIMPLE| SIMPLE_CAR-> #IGNORE + #NEXT + NEW_STD
CONT_SIMPLE| DOUBLE_CAR-> CONT_SIMPLE
CONT_SIMPLE| PIPE_CAR  -> CONT_SIMPLE
CONT_SIMPLE| AMP_CAR   -> CONT_SIMPLE
CONT_SIMPLE| LT_CAR    -> CONT_SIMPLE
CONT_SIMPLE| GT_CAR    -> CONT_SIMPLE

NEW_DOUBLE| SPE_CAR   -> CONT_DOUBLE
NEW_DOUBLE| STD_CAR   -> CONT_DOUBLE
NEW_DOUBLE| SIMPLE_CAR-> #IGNORE + #NEXT + NEW_STD
NEW_DOUBLE| DOUBLE_CAR-> CONT_DOUBLE
NEW_DOUBLE| PIPE_CAR  -> CONT_DOUBLE
NEW_DOUBLE| AMP_CAR   -> CONT_DOUBLE
NEW_DOUBLE| LT_CAR    -> CONT_DOUBLE
NEW_DOUBLE| GT_CAR    -> CONT_DOUBLE

CONT_DOUBLE| SPE_CAR   -> CONT_DOUBLE
CONT_DOUBLE| STD_CAR   -> CONT_DOUBLE
CONT_DOUBLE| SIMPLE_CAR-> #IGNORE + #NEXT + NEW_STD
CONT_DOUBLE| DOUBLE_CAR-> CONT_DOUBLE
CONT_DOUBLE| PIPE_CAR  -> CONT_DOUBLE
CONT_DOUBLE| AMP_CAR   -> CONT_DOUBLE
CONT_DOUBLE| LT_CAR    -> CONT_DOUBLE
CONT_DOUBLE| GT_CAR    -> CONT_DOUBLE

REDIR_IN| SPE_CAR   -> REDIR_IN + NEW_STD
REDIR_IN| STD_CAR   -> REDIR_IN + CONT_STD
REDIR_IN| SIMPLE_CAR-> #IGNORE + REDIR_IN + NEW_SIMPLE
REDIR_IN| DOUBLE_CAR-> #IGNORE + REDIR_IN + NEW_DOUBLE
REDIR_IN| PIPE_CAR  -> SYNTAX_ERROR
REDIR_IN| AMP_CAR   -> 
REDIR_IN| LT_CAR    -> HERE_DOC
REDIR_IN| GT_CAR    -> REDIR_IN x

HEREDOC   | SPE_CAR   -> #IGNORE + HEREDOC
HEREDOC   | STD_CAR   -> HEREDOC + CONT_STD
HEREDOC   | SIMPLE_CAR-> HEREDOC + CONT_SIMPLE
HEREDOC   | DOUBLE_CAR-> HEREDOC + CONT_DOUBLE
HEREDOC   | PIPE_CAR  -> SYNTAX_ERROR
HEREDOC   | AMP_CAR   -> 
HEREDOC   | LT_CAR    -> SYNTAX_ERROR x
HEREDOC   | GT_CAR    -> SYNTAX_ERROR x

REDIR_OUT | SPE_CAR   -> #IGNORE + REDIR_OUT
REDIR_OUT | STD_CAR   -> REDIR_OUT + CONT_STD
REDIR_OUT | SIMPLE_CAR-> REDIR_OUT + CONT_SIMPLE
REDIR_OUT | DOUBLE_CAR-> REDIR_OUT + CONT_DOUBLE
REDIR_OUT | PIPE_CAR  -> SYNTAX_ERROR
REDIR_OUT | AMP_CAR   -> 
REDIR_OUT | LT_CAR    -> SYNTAX_ERROR x
REDIR_OUT | GT_CAR    -> REDIR_OUT_APP

REDIR_OUT_APP| SPE_CAR   -> #IGNORE + REDIR_OUT_APP
REDIR_OUT_APP| STD_CAR   -> REDIR_OUT_APP + CONT_STD
REDIR_OUT_APP| SIMPLE_CAR-> REDIR_OUT_APP + CONT_SIMPLE
REDIR_OUT_APP| DOUBLE_CAR-> REDIR_OUT_APP + CONT_DOUBLE
REDIR_OUT_APP| PIPE_CAR  -> SYNTAX_ERROR
REDIR_OUT_APP| AMP_CAR   -> 
REDIR_OUT_APP| LT_CAR    -> SYNTAX_ERROR x
REDIR_OUT_APP| GT_CAR    -> SYNTAX_ERROR x

PIPE...
AMP...
